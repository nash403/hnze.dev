{
  "name": "hnze-dev",
  "type": "module",
  "private": true,
  "version": "2.0.0",
  "scripts": {
    "clean:nuxt": "bun run --bun nuxt cleanup",
    "clean:content": "rm -rf .data",
    "clean:deps": "rm -rf node_modules",
    "clean:all": "bunx run-s clean:nuxt clean:content clean:deps",
    "build": "bun run --bun nuxt build --prerender",
    "dev": "bun run --bun nuxt dev",
    "generate": "bun run --bun nuxt generate",
    "preview": "bun run --bun nuxt preview",
    "serve": "npx serve .vercel/output/static ",
    "deploy": "bunx vercel deploy --archive=tgz --logs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "bun run --bun nuxt prepare",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@iconify-json/flagpack": "^1.2.6",
    "@iconify-json/line-md": "^1.2.11",
    "@iconify-json/logos": "^1.2.10",
    "@iconify-json/mingcute": "^1.2.5",
    "@nuxt/content": "3.8.2",
    "@nuxt/eslint": "1.10.0",
    "@nuxt/fonts": "0.12.1",
    "@nuxt/icon": "2.1.0",
    "@nuxt/image": "2.0.0",
    "@nuxt/scripts": "0.13.0",
    "@nuxtjs/color-mode": "4.0.0",
    "@nuxtjs/i18n": "10.2.1",
    "@nuxtjs/seo": "3.2.2",
    "@posthog/nuxt": "1.3.2",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.17",
    "@unhead/vue": "^2.0.19",
    "@vueuse/components": "^14.0.0",
    "@vueuse/nuxt": "14.0.0",
    "better-sqlite3": "^12.4.6",
    "daisyui": "^5.5.5",
    "nuxt": "^4.2.1",
    "tailwindcss": "^4.1.17",
    "unplugin-info": "1.2.4",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@intlify/eslint-plugin-vue-i18n": "^4.1.0",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "eslint-plugin-tailwindcss": "npm:@hyoban/eslint-plugin-tailwindcss@4.0.0-alpha.13",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3",
    "unplugin-icons": "^22.5.0",
    "zod": "^4.1.13",
    "zod-to-json-schema": "^3.25.0"
  }
}
